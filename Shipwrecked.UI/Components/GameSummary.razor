@using Shipwrecked.Infrastructure.Interfaces
<h1 class="header">Day @GameState.Day</h1>

@code {
    [Inject] 
    private IContext Context { get; set; } = default!;

    private Game GameState { get; set; } = default!;

    protected override void OnInitialized()
    {
        GameState = Context.GetState().Game;        
        base.OnInitialized();
    }

}